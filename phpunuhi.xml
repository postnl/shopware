<phpunuhi
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="./vendor/boxblinkracer/phpunuhi/config.xsd"
>
    <translations>

        <set name="Storefront JSON">
            <format>
                <json indent="2" sort="true"/>
            </format>
            <locales>
                <locale name="en-GB">./src/Resources/snippet/en_GB/storefront.en-GB.json</locale>
                <locale name="de-DE">./src/Resources/snippet/de_DE/storefront.de-DE.json</locale>
                <locale name="nl-NL">./src/Resources/snippet/nl_NL/storefront.nl-NL.json</locale>
            </locales>
        </set>

    </translations>
</phpunuhi>
