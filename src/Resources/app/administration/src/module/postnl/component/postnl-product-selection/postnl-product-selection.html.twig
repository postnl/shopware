
<sw-container>
    {{ product.productCodeDelivery }} {{ product.name }}

    <sw-single-select v-model="actualDeliveryType"
                      :options="availableDeliveryTypes"
                      :disabled="availableDeliveryTypes.length == 0"
                      @change="onChangeDeliveryType"/>

    <sw-container columns="1fr 1fr" gap="0 16px">
        <sw-switch-field v-for="availableFlag in availableFlags"
                         v-if="availableFlag.visible"
                         v-model="flags[availableFlag.name]"
                         :key="availableFlag.name"
                         :label="availableFlag.name"
                         :disabled="availableFlag.disabled"
                         @change="onChangeFlag(availableFlag.name)"
                         size="small"
                         noMarginTop/>
    </sw-container>
</sw-container>

