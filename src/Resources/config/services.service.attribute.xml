<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <defaults public="true"/>

        <service id="PostNL\Shipments\Service\Attribute\AttributeFactory">
            <argument type="tagged" tag="postnl.attribute.handler.entity"/>
            <argument type="tagged" tag="postnl.attribute.handler.type"/>
            <argument type="service" id="postnl_shipments.logger"/>
        </service>
    </services>
</container>
